{
	"name": "Toolkit-for-VIEW",
	"dockerComposeFile": "docker-compose.yml",
	"service": "app",
	"workspaceFolder": "/workspace",
	"forwardPorts": [
		9443
	],
	"customizations": {
		"vscode": {
			"settings": {
				"json.schemas": [
					{
						"fileMatch": [
							"/manifest.json"
						],
						"url": "https://json.schemastore.org/chrome-manifest"
					}
				],
				"editor.formatOnSave": true,
				"files.eol": "\n"
			}
		}
	}
}